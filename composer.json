{
    "name":         "vnagara/geoip-module",
    "type":         "library",
    "description":  "Geoip module for zend framework application to store Record. It utilize maxmind database and API",
    "keywords":     ["geoip", "zf", "zf2", "zend framework"],
    "license":      "BSD-3-Clause",
    "authors": [
        {"name": "Vitalii Nagara", "email": "duke@ejoom.com", "homepage": "http://www.maxmind.com/"}
    ],

    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],

    "require": {
        "php":                              ">=5.3.3",
        "geoip/geoip":                      "~1.13",
        "zendframework/zend-stdlib":        "~2.2.0"
    },

    "autoload": {
        "psr-0": {"GeoipModule\\": "src/"},
        "classmap": ["Module.php"]
    }
}
